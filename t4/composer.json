{
  "name": "pr-of-it/t4",
  "description": "T4 framework",
  "keywords": [
    "php",
    "framework",
    "t4"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "twig/twig": "1.*",
    "phpmailer/phpmailer": "5.*",
    "psr/log": "1.*",
    "react/http": "^0.8.4",
    "react/child-process": "^0.6.1"
  },
  "require-dev": {
    "phing/phing": "2.*",
    "phpunit/phpunit": "3.7.*",
    "phpunit/dbunit": "1.3.*"
  },
  "autoload": {
    "psr-4": {
      "T4\\": "framework/"
    },
    "files": [
      "framework/boot.php"
    ]
  }
}
